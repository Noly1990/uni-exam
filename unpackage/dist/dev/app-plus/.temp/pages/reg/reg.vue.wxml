<template name="132c6e40">
  <view class="_view data-v-d8b44fca content">
    <view class="_view data-v-d8b44fca input-group">
      <view class="_view data-v-d8b44fca input-row border">
        <text class="_text data-v-d8b44fca title">学号：</text>
        <input type="text" value="{{stuId}}" bindinput="handleProxy" placeholder="请输入学号" data-eventid="{{'0'}}" data-comkey="{{$k}}" class="_input data-v-d8b44fca" />
      </view>
      <view class="_view data-v-d8b44fca input-row border">
        <text class="_text data-v-d8b44fca title">姓名：</text>
        <input type="text" value="{{realName}}" bindinput="handleProxy" placeholder="请输入姓名" data-eventid="{{'1'}}" data-comkey="{{$k}}" class="_input data-v-d8b44fca" />
      </view>
      <view class="_view data-v-d8b44fca input-row border">
        <text class="_text data-v-d8b44fca title">密码：</text>
        <input type="text" password="true" value="{{password}}" bindinput="handleProxy" placeholder="请输入密码" data-eventid="{{'2'}}" data-comkey="{{$k}}" class="_input data-v-d8b44fca" />
      </view>
      <view class="_view data-v-d8b44fca input-row border">
        <text class="_text data-v-d8b44fca title">性别：</text>
        <radio-group class="_radio-group data-v-d8b44fca radio-group" bindchange="handleProxy" data-eventid="{{'3'}}" data-comkey="{{$k}}">
          <label class="_label data-v-d8b44fca radio" wx:key="item" key="{{item}}" wx:for="{{items}}" wx:for-index="index" wx:for-item="item">
            <radio value="{{item.name}}" checked="{{item.checked}}" class="_radio data-v-d8b44fca" />{{item.value}} </label>
        </radio-group>
      </view>
      <view class="_view data-v-d8b44fca input-row border">
        <view class="_view data-v-d8b44fca title">日期：</view>
        <view class="_view data-v-d8b44fca"> {{year}}年{{month}}月{{day}}日 </view>
      </view>
    </view>
    <view class="_view data-v-d8b44fca">
      <picker-view class="_picker-view data-v-d8b44fca picker-view" indicator-style="{{indicatorStyle}}" value="{{value}}" bindchange="handleProxy" data-eventid="{{'4'}}" data-comkey="{{$k}}">
        <picker-view-column class="_picker-view-column data-v-d8b44fca">
          <view class="_view data-v-d8b44fca picker-item" wx:key="item" key="{{item}}" wx:for="{{years}}" wx:for-index="index" wx:for-item="item">{{item}}年</view>
        </picker-view-column>
        <picker-view-column class="_picker-view-column data-v-d8b44fca">
          <view class="_view data-v-d8b44fca picker-item" wx:key="item" key="{{item}}" wx:for="{{months}}" wx:for-index="index" wx:for-item="item">{{item}}月</view>
        </picker-view-column>
        <picker-view-column class="_picker-view-column data-v-d8b44fca">
          <view class="_view data-v-d8b44fca picker-item" wx:key="item" key="{{item}}" wx:for="{{days}}" wx:for-index="index" wx:for-item="item">{{item}}日</view>
        </picker-view-column>
      </picker-view>
    </view>
    <view class="_view data-v-d8b44fca btn-row">
      <button type="primary" class="_button data-v-d8b44fca primary" bindtap="handleProxy" data-eventid="{{'5'}}" data-comkey="{{$k}}">注册</button>
    </view>
  </view>
</template>