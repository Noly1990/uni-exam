<template name="78e76e9b">
  <view class="_view data-v-ef5b76ac question-box">
    <view class="_view data-v-ef5b76ac question-title">
      <text class="_text data-v-ef5b76ac title-index">第{{questionObj.qindex}}题：</text>
      <text class="_text data-v-ef5b76ac">{{questionObj.qtitle}}</text>
    </view>
    <view class="_view data-v-ef5b76ac answer-box">
      <view bindtap="handleProxy" class="_view data-v-ef5b76ac answer-item {{item.className}}" wx:key="item.index" key="{{item.index}}" data-eventid="{{'DcR-0-'+index}}" data-comkey="{{$k}}" wx:for="{{optionArr}}" wx:for-index="index" wx:for-item="item">
        <view wx:if="{{item.show}}" class="_view data-v-ef5b76ac"> {{item.face}}:{{item.title}} </view>
      </view>
    </view>
  </view>
</template>